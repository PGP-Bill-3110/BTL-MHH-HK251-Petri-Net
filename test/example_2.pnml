<?xml version="1.0" encoding="UTF-8"?>
<pnml xmlns="http://www.pnml.org/version-2010/grammar/pnml">

  <!-- ================== Original Net ================== -->
  <net id="n2" type="http://www.pnml.org/version-2010/grammar/ptnet">
    <name>
      <text>Producer-Consumer Cycle (1-Safe)</text>
    </name>
    <page id="p1">
      <place id="P1">
        <name><text>Prod_Ready</text></name>
        <initialMarking><text>1</text></initialMarking>
        <graphics><position x="123" y="87"/></graphics>
      </place>
      <place id="P2">
        <name><text>Buff_Empty</text></name>
        <initialMarking><text>1</text></initialMarking>
        <graphics><position x="174" y="56"/></graphics>
      </place>
      <place id="P3">
        <name><text>Buff_Full</text></name>
        <initialMarking><text>0</text></initialMarking>
        <graphics><position x="98" y="135"/></graphics>
      </place>
      <place id="P4">
        <name><text>Cons_Ready</text></name>
        <initialMarking><text>1</text></initialMarking>
        <graphics><position x="160" y="142"/></graphics>
      </place>

      <transition id="T1">
        <name><text>Produce</text></name>
        <graphics><position x="145" y="75"/></graphics>
      </transition>
      <transition id="T2">
        <name><text>Consume</text></name>
        <graphics><position x="110" y="110"/></graphics>
      </transition>

      <arc id="A1" source="P1" target="T1"><inscription><text>1</text></inscription></arc>
      <arc id="A2" source="P2" target="T1"><inscription><text>1</text></inscription></arc>
      <arc id="A3" source="T1" target="P3"><inscription><text>1</text></inscription></arc>
      <arc id="A4" source="T1" target="P1"><inscription><text>1</text></inscription></arc>
      <arc id="A5" source="P3" target="T2"><inscription><text>1</text></inscription></arc>
      <arc id="A6" source="P4" target="T2"><inscription><text>1</text></inscription></arc>
      <arc id="A7" source="T2" target="P2"><inscription><text>1</text></inscription></arc>
      <arc id="A8" source="T2" target="P4"><inscription><text>1</text></inscription></arc>
    </page>
  </net>

